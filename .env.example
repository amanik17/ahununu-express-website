# Copy this file to .env and update the values
NODE_ENV=development

# Public site URL used in structured data and meta (no trailing slash)
REACT_APP_PUBLIC_SITE_URL=https://ahununu.com

# Frontend API base URL (can be relative like /api when using a reverse proxy)
REACT_APP_API_BASE_URL=/api

# Optional: privacy-friendly analytics domain (e.g., ahununu.com if using Plausible)
REACT_APP_ANALYTICS_DOMAIN=

# Optional: Sentry DSN for error monitoring (leave empty to disable)
REACT_APP_SENTRY_DSN=
