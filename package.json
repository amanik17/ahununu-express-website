{
  "name": "ahununu-express-website",
  "version": "1.0.0",
  "description": "Ahununu Express - Trusted Express Service Provider",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "dev": "webpack serve --mode development --open --config webpack.config.js",
    "analyze": "cross-env ANALYZE=true webpack --mode production --config webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@react-spring/three": "^10.0.2",
    "@react-spring/web": "^10.0.2",
    "@react-three/cannon": "^6.6.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/gltfjsx": "^4.3.4",
    "@sentry/react": "^10.12.0",
    "@sentry/tracing": "^7.120.4",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1",
    "three": "^0.155.0",
    "three-mesh-bvh": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^30.1.2",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "html-webpack-plugin": "^5.6.5",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "style-loader": "^3.3.0",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": [
        "babel-jest",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ]
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "keywords": [
    "logistics",
    "express",
    "delivery",
    "react",
    "website"
  ],
  "author": "Ahununu Express",
  "license": "MIT"
}
